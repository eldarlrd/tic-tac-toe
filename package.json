{
  "name": "tic-tac-toe",
  "private": true,
  "version": "1.4.1",
  "type": "module",
  "keywords": [
    "tic-tac-toe",
    "web game"
  ],
  "description": "A classic Tic Tac Toe game",
  "repository": "github:eldarlrd/tic-tac-toe",
  "homepage": "https://eldarlrd.is-a.dev/tic-tac-toe",
  "author": "Eldar Pashazade <eldarlrd@pm.me> (https://eldarlrd.is-a.dev)",
  "license": "AGPL-3.0-only",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "deployer": "bun bundle && gh-pages -b prod -m 'build(prod): deployment' -d dist",
    "lint": "eslint src",
    "style": "stylelint 'src/**/*.{css,pcss,scss,sass,less,styl}'",
    "pretty": "prettier '{src,__{tests,mocks,snapshots}__}/**/*.{ts,tsx,scss}' --write",
    "format": "bun lint --fix && bun pretty",
    "cut": "knip --fix --cache"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "experimentalTernaries": true
  },
  "dependencies": {
    "@fontsource/patrick-hand": "^5.2.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tachyons": "^4.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "knip": "^5.63.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "workbox-core": "^7.3.0",
    "workbox-precaching": "^7.3.0"
  }
}
