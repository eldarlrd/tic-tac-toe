{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://eldarlrd.github.io/tic-tac-toe",
  "scripts": {
    "start": "inferno-scripts --openssl-legacy-provider start",
    "build": "inferno-scripts --openssl-legacy-provider build",
    "pretty": "prettier --write 'src/**/*.js'",
    "lint": "eslint src --ext js --report-unused-disable-directives",
    "format": "yarn pretty && yarn lint --fix",
    "deploy": "yarn build && gh-pages -b deploy -m 'build: Deployment' -d build",
    "eject": "inferno-scripts eject"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "inferno": "^8.2.2",
    "inferno-hyperscript": "^8.2.2",
    "inferno-scripts": "7.0.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.9",
    "eslint-config-prettier": "^8.8.0",
    "gh-pages": "^5.0.0",
    "prettier": "^3.0.0",
    "tachyons": "^4.12.0"
  },
  "eslintConfig": {
    "extends": "inferno-app"
  },
  "eslintIgnore": [
    "index.js"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
